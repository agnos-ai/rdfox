PREFIX ub: <http://swat.cse.lehigh.edu/onto/univ-bench.owl#>

SELECT ?S ?P ?O ?G 
WHERE { 
    { 
        SELECT * { GRAPH ?G {
                ?S ?P ?O
            } 
            FILTER(?G = ub:data)
        }
        LIMIT 5
    }
    UNION
    {
        SELECT * { GRAPH ?G {
                ?S ?P ?O
            }     
            FILTER(?G = ub:ontology)
        }
        LIMIT 5
    }
}
